{
  "name": "TinyLara/TinyLara",
  "description": "A Simple PHP Framework based on Composer, looks like a Tiny Laravel.",
  "keywords": ["tiny", "simple", "framework", "laravel"],
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "tinylara/framework": "dev-master",
    "illuminate/database": "^5.4",
    "danielstjules/stringy": "~3.0.1",
    "filp/whoops": "^2.1",
    "monolog/monolog": "^1.22",
    "nette/mail": "^2.4",
    "predis/predis": "^1.1",
    "php-amqplib/php-amqplib": "2.6.*"
  },
  "autoload": {
    "psr-4": {
        "App\\": "app/",
        "Tests\\": "tests/"
    },
    "classmap": [
      "services",
      "app/Controller",
      "app/Jobs",
      "app/Model",
      "app/Workers",
      "app/Repositories"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.9"
  }
}
